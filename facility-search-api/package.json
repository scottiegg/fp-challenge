    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.6",
    "@types/supertest": "^6.0.2",
    "typescript": "^5.9.3",
    "tsx": "^4.7.0",
    "vitest": "^4.0.13",
    "@vitest/coverage-v8": "^4.0.13",
    "supertest": "^7.1.4",
    "@eslint/js": "^9.39.1",
    "eslint": "^9.39.1",
    "typescript-eslint": "^8.47.0"
  }
