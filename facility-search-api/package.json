{
  "name": "facility-search-api",
  "version": "1.0.0",
  "description": "RESTful API for searching and filtering fitness facilities",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
